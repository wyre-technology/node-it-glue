{
  "name": "@asachs01/node-it-glue",
  "version": "0.1.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "description": "Comprehensive, fully-typed Node.js/TypeScript library for the IT Glue API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asachs01/node-it-glue.git"
  },
  "keywords": [
    "it-glue",
    "itglue",
    "kaseya",
    "msp",
    "api",
    "typescript",
    "json-api"
  ],
  "author": "Aaron Sachs / WYRE Technology",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/asachs01/node-it-glue/issues"
  },
  "homepage": "https://github.com/asachs01/node-it-glue#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@types/node": "^25.2.0",
    "msw": "^2.12.8",
    "semantic-release": "^24.2.9",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "undici": "^7.20.0"
  }
}
